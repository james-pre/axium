		"./components/*": "./lib/*.svelte",
		"./styles/*": "./styles/*.css"
	},
	"files": [
		"dist",
		"lib",
		"build",
		"routes"
	],
	"scripts": {
		"build": "tsc"
	},
	"peerDependencies": {
		"@axium/client": ">=0.4.4",
		"@axium/core": ">=0.7.0",
		"@axium/server": ">=0.23.0",
		"@sveltejs/kit": "^2.27.3",
		"utilium": "^2.3.8"
	},
	"dependencies": {
		"blakejs": "^1.2.1",
		"zod": "^4.0.5"
	},
	"axium": {
		"http_handler": "./build/handler.js",
		"hooks": "./dist/hooks.js",
		"routes": "./routes",
		"apps": [
			{
				"id": "files",
				"name": "Files",
				"icon": "folders"
			}
		]
	}
